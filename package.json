{
  "name": "clainjo-horror",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node server/index.js",
    "dev:all": "concurrently -n client,server -c auto \"npm run dev\" \"npm run server\"",
    "start": "node server/index.js",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "maj": "docker compose down || true && docker rm -f clainjo-horror || true && git pull && docker compose up -d --build --remove-orphans",
    "generate-icons": "node scripts/generate-icons.js",
    "generate-placeholder-icons": "node scripts/generate-placeholder-icons.js",
    "pwa:test": "npm run build && npm run preview"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.26.2",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "three": "^0.161.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "vite": "^7.1.6",
    "concurrently": "^9.1.0",
    "vitest": "^2.1.4"
  }
}
